<script>
  // Initialize sorting dropdown
  $('[data-ui-behavior=dropdown-sort]')
    .dropdown({
      action: 'select'
    })
  ;

  // Initialize regular dropdowns
  $('[data-ui-behavior=dropdown]')
    .dropdown()
  ;

  // Initialize more actions dropdown
  $('[data-ui-behavior=more-actions-dropdown]')
    .dropdown({
      action: 'nothing'
    })
  ;

  // Initialize grade select popup
  $('[data-ui-behavior=mass-update-grade-popup-trigger]')
    .popup({
      on: 'click',
      popup: '[data-ui-behavior=mass-update-grade-popup]'
    })
  ;

  // Initialize select all popover
  $('[data-ui-behavior=select-all-checkbox-popover]')
    .popup({
      on: 'click'
    })
  ;

  // Initialize merge modal
  $('[data-ui-behavior=merge-modal]')
    .modal('attach events', '[data-ui-behavior=merge-button]', 'show')
  ;

  // Initialize delete confirmation modal
  $('[data-ui-behavior=delete-contacts-modal]')
    .modal('attach events', '[data-ui-behavior=delete-contacts-button]', 'show')
  ;

  // Initialize all campaigns modal
  $('[data-ui-behavior=all-campaigns-modal]')
    .modal('attach events', '[data-ui-behavior=see-all-campaigns-link]', 'show')
  ;

  // Initialize accordion
  $('[data-ui-behavior=accordion-filter]')
    .accordion({
      exclusive: false,
      duration: 100
    })
  ;

  // Initialize Semantic UI dropdowns
  $('[data-ui-behavior=checkbox]')
    .checkbox()
  ;

  // Initialize Semantic UI calendar component for date selection
  $('[data-ui-behavior=calendar_date]')
    .calendar({
      type: 'date',
      today: true,
    })
  ;

  // Initialize grade select popup
  $('[data-ui-behavior=grade-select-popup-trigger]')
    .popup({
      on: 'click'
    })
  ;

  // Initialize sticky
  $('[data-ui-behavior=contacts-select-menu]')
    .sticky({
      offset: 56
    })
  ;
</script>
